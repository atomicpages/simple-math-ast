{
  "name": "simple-math-ast",
  "version": "1.0.0",
  "description": "Simple Math AST",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist es",
    "build": "npm run build:commonjs && npm run build:umd && npm run build:umd:min && npm run build:es",
    "build:watch": "echo 'build && watch the COMMONJS version of the package - for other version, run specific tasks' && npm run build:commonjs:watch",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:commonjs:watch": "npm run build:commonjs -- --watch",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:es:watch": "npm run build:es -- --watch",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development node_modules/.bin/rollup src/index.js --config --sourcemap --output dist/simple-math-ast.js",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup src/index.js --config --output dist/simple-math-ast.min.js",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "math-ast"
  ],
  "author": "Esimov Miras",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Flyr1Q/simple-math-ast"
  },
  "bugs": {
    "url": "https://github.com/Flyr1Q/simple-math-ast/issues"
  },
  "dependencies": {
    "ramda": "0.25.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.4",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "cross-env": "^5.1.3",
    "eslint": "4.18.2",
    "eslint-config-unobtrusive": "1.2.2",
    "eslint-plugin-import": "^2.9.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-watch": "^4.3.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  }
}
