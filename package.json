{
  "name": "@djthoms/simple-math-ast",
  "version": "1.0.0",
  "description": "Simple Math AST",
  "keywords": [
    "math-ast",
    "math",
    "math syntax tree",
    "syntax tree"
  ],
  "bugs": {
    "url": "https://github.com/atomicpages/simple-math-ast/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atomicpages/simple-math-ast"
  },
  "license": "MIT",
  "author": "Esimov Miras",
  "contributors": [
    "Dennis Thompson"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./tokenizer": {
      "types": "./dist/types/tokenize/index.d.ts",
      "import": "./dist/esm/tokenize/index.js",
      "require": "./dist/cjs/tokenize/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npm-run-all -p build:*",
    "lint": "eslint --ext .ts,.js src",
    "benchmark": "vitest bench --run",
    "test": "vitest run",
    "test:watch": "vitest",
    "build:cjs": "esbuild --target=es2020 --outdir=dist/cjs --sourcemap --format=cjs $(./scripts/fd.sh)",
    "build:esm": "esbuild --target=es2020 --outdir=dist/esm --sourcemap --format=esm $(./scripts/fd.sh)",
    "build:types": "tsc -p tsconfig.build.json",
    "check-types": "tsc --noEmit --emitDeclarationOnly false",
    "prepare": "husky install",
    "pre-push": "npm-run-all -p check-types lint test"
  },
  "devDependencies": {
    "@djthoms/eslint-config": "^6.1.0",
    "@djthoms/prettier-config": "^4.3.0",
    "@vitest/coverage-v8": "^0.34.4",
    "esbuild": "^0.19.2",
    "fd-find": "^1.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  }
}
